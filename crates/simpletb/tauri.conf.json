{
  "productName": "simpletb",
  "version": "0.0.0",
  "identifier": "com.tauri.dev",
  "build": {
    "beforeDevCommand": "pnpm dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "pnpm build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "label": "main",
        "title": "simpletb",
        "url": "/crates/ui/src/menubar/index.html",
        "height": 22,
        "alwaysOnBottom": false,
        "alwaysOnTop": true,
        "decorations": false,
        "transparent": true,
        "closable": false,
        "maximizable": false,
        "minimizable": true,
        "resizable": false,
        "skipTaskbar": true,
        "focus": false,
        "shadow": false,
        "x": 0,
        "y": 0
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
